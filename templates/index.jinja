<!--content-->
<div class="content">
	<div class="container">
		<div class="content-top">
			<h1>Recent Products</h1>
			<div class="content-top1">
		{% for store in records.items %}
        	<div class="col-md-3 col-md2">
					<div class="col-md1 simpleCart_shelfItem">
						<a href="{{ url_for('viewsProduct', id=store.id) }}">
                         {% if os.path.isfile(os.path.join(app.config['UPLOAD_FOLDER']+'/image', store.picture)) %}
							<img class="img-responsive" src="{{ url_for('static', filename='download/image/'+store.picture) }}" alt="" />
                         {% else %}
                            <img class="img-responsive" src="{{ url_for('static', filename='download/image/notdata.png') }}" alt="" />
                         {% endif %}
						</a>
						<h3><a href="{{ url_for('viewsProduct', id=store.id) }}">{{ store.product }}</a></h3>
						 <div class="price">
								<h5 class="item_price">{{ ConvertMany(store.price, '.') }}</h5>
								<a class="item_add" href="{{ url_for('buyProduct', id=store.id) }}">Add To Cart</a>
								<div class="clearfix"> </div>
						 </div>
						
					</div>
			</div>
        {% endfor %}

