{% if records %}
{% for message in get_flashed_messages() %}
{{ message }}
{% endfor %}
{% for store in records.items %}
{% if store.product == Not %}
{{ messages }}
{% endif %}
<p>Product : {{ store.product }}</p>
<p>Price : {{ convert(store.price, '.') }} IDR</p>
<p>Descrip : {{ store.descrip }}</p>
{% endfor %}
{% else %}}
{{ messages }}
{% endif %}
{% if categorys: %}

  {% if records.has_prev %}
  <a href="{{ url_for( "store", page=records.next_num, categorys=categorys ) }}">Previous<a>
  {% else %}
  <a href="#">Previous<a>
  {% endif %}

  {% for num in records.iter_pages(left_edge=2,   left_current=5,  right_current=5 ,right_edge=2) %}
  {% if num %}
     {% if num== records.num %}
     <a href="{{ url_for( "store", categorys=categorys ) }}">{{ num }}<a>
     {% else %}
     <a href="{{ url_for( "store", categorys=categorys ) }}">{{ num }}<a>
     {% endif %}
  {% else %}
  <a href="#">...<a>
  {% endif %}
  {% endfor %}

  {% if records.has_next %}
  <a href="{{ url_for( "store", page=records.prev_num,  category=categorys  ) }}">Next<a>
  {% else %}
  <a href="#">Next<a>
  {% endif %}

{% elif brands: %}
 
  {% if records.has_prev %}
