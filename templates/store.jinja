  {% else %}
  <a href="#">...<a>
  {% endif %}
  {% endfor %}

  {% if records.has_next %}
  <a href="{{ url_for( "store", page=records.prev_num,  brands=brands  ) }}">Next<a>
  {% else %}
  <a href="#">Next<a>
  {% endif %}

{% else %}
  {% if records.has_prev %}
