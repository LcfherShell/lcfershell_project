{% with messages = get_flashed_messages() %}
           {% if messages %}
          {% for message in messages %}
            <div class="alert alert-success">
             <strong>{{ message }}</strong>
           </div>
          {% endfor %}
           {% endif %}
{% endwith %}
<form method="post" action="" enctype="multipart/form-data">
                        
    <input type="file" name="files" multiple="true" accept="image/*" autocomplete="off" required>

    <p>product</p>
    <input type="text" name="product" value="{{ product }}" autocomplete="off">
    <p>brand</p>
    <input type="text" name="brand"  value="{{ brands }}" autocomplete="off"> 
    <p>category</p>
    <select name="category">
    {% for category in listcategory %}
     <option value="{{ category.category }}">{{ category.category }}</option>
    {% endfor %}
   </select>
