<div class="single">
{% with messages = get_flashed_messages() %}
  {% if messages %}
    <ul class=flashes>
    {% for message in messages %}
      <li>{{ message }}</li>
    {% endfor %}
    </ul>
  {% endif %}
{% endwith %}

{% if seller %}
<div class="container">
<div class="col-md-9">
	<div class="col-md-5 grid">
  {% if os.path.isfile(os.path.join(app.config['UPLOAD_FOLDER']+'/image', picture)) %}
     <img src="{{ url_for('static', filename='download/image/'+picture) }}" data-imagezoom="true" class="img-responsive">
  {% else %}
    <img class="img-responsive" src="{{ url_for('static', filename='download/image/notdata.png') }}" alt="" />
  {% endif %}
	</div>	
<div class="col-md-7 single-top-in">
						<div class="single-para simpleCart_shelfItem">
							<h1>{{ product }}</h1>
							<p><b>Seller: </b>{{ seller }}</p>
              <p><b>Brand: </b>{{ brand }}</p>
              <p><b>Stock: </b>{{ stock }}</p>
              <p><b>Uploud: </b>{{ uploud }}</p>
              <center><p><b>Description</b></p></center>
              <p>{{ description }}</p>
							<label  class="add-to item_price">{{ price }} <a href="{{ url_for('buyProduct', id=idproduct) }}" class="item_add"><small>Add To Cart</small></a></label>
							
					</div>
			<div class="clearfix"> </div>
			<div class="content-top1">
      {% for stores in records.items %}
       {% if not stores.id == idproduct %}
      <div class="col-md-4 col-md3">
					<div class="col-md1 simpleCart_shelfItem">
						<a href="{{ url_for('viewsProduct', id=stores.id) }}">
							{% if os.path.isfile(os.path.join(app.config['UPLOAD_FOLDER']+'/image', stores.picture)) %}
                <img src="{{ url_for('static', filename='download/image/'+stores.picture) }}" data-imagezoom="true" class="img-responsive">
              {% else %}
                <img class="img-responsive" src="{{ url_for('static', filename='download/image/notdata.png') }}" alt="" />
						  {% endif %}
            </a>
						<h3><a href="{{ url_for('viewsProduct', id=stores.id) }}">{{ stores.product }}</a></h3>
            <h5 class="item_price">{{ ConvertMany(stores.price, '.') }} USD</h5>
						<div class="price">	
								<a href="{{ url_for('buyProduct', id=stores.id) }}" class="item_add">Add To Cart</a>
								<div class="clearfix"> </div>
						</div>
					</div>
			</div>
      {% endif %}	
      {% endfor %}	
        </div>
		</div>
    	</div>
</div>
		<div class="clearfix"> </div>

</div>	

     {% else %}
{% with messages = get_flashed_messages() %}
           {% if messages %}
          {% for message in messages %}
            <div class="alert alert-success">
             <strong>{{ message }}</strong>
           </div>
          {% endfor %}
           {% endif %}
{% endwith %}
{% endif %}	

